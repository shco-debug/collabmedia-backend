{
	"info": {
		"_postman_id": "birthday-stream-api",
		"name": "CollabMedia Birthday Stream API",
		"description": "Complete API collection for Birthday Stream functionality including member management, questions, answers, and delivery system",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "1. Stream Setup & Publishing",
			"item": [
				{
					"name": "Publish Birthday Stream",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "capsule_id",
								"value": "{{capsule_id}}",
								"description": "ID of the existing capsule to publish"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"makingFor\": \"OTHERS\",\n  \"CapsuleFor\": \"Stream\",\n  \"StreamType\": \"Group\",\n  \"StreamFlow\": \"Birthday\",\n  \"LaunchSettings\": {\n    \"OwnerBirthday\": \"1990-05-15\",\n    \"Audience\": \"OTHERS\",\n    \"ShareMode\": \"friend-group\"\n  },\n  \"Frequency\": \"medium\",\n  \"MonthFor\": \"M12\",\n  \"price\": 29.99,\n  \"DiscountPrice\": 0\n}"
						},
						"url": {
							"raw": "{{base_url}}/capsules/publish",
							"host": ["{{base_url}}"],
							"path": ["capsules", "publish"]
						},
						"description": "Publishes and launches your existing capsule as a birthday stream with group settings"
					}
				},
				{
					"name": "Check Capsule Completeness",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "capsule_id",
								"value": "{{capsule_id}}"
							}
						],
						"url": {
							"raw": "{{base_url}}/capsules/capsule__checkCompleteness",
							"host": ["{{base_url}}"],
							"path": ["capsules", "capsule__checkCompleteness"]
						},
						"description": "Checks if capsule is ready for publishing"
					}
				}
			]
		},
		{
			"name": "2. Member Management",
			"item": [
				{
					"name": "Add Members to Stream",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"StreamId\": \"{{stream_id}}\",\n  \"OwnerId\": \"{{owner_id}}\",\n  \"members\": [\n    {\n      \"Email\": \"friend1@example.com\",\n      \"Name\": \"Alice Johnson\"\n    },\n    {\n      \"Email\": \"friend2@example.com\",\n      \"Name\": \"Bob Smith\"\n    },\n    {\n      \"Email\": \"friend3@example.com\",\n      \"Name\": \"Carol Davis\"\n    }\n  ]\n}"
						},
						"url": {
							"raw": "{{base_url}}/stream__addMembers",
							"host": ["{{base_url}}"],
							"path": ["stream__addMembers"]
						},
						"description": "Adds members to the birthday stream and sends invitation emails"
					}
				},
				{
					"name": "Get Stream Members List",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"StreamId\": \"{{stream_id}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/stream__getMembersList",
							"host": ["{{base_url}}"],
							"path": ["stream__getMembersList"]
						},
						"description": "Retrieves list of all members in the stream"
					}
				},
				{
					"name": "Update Stream Members",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"StreamId\": \"{{stream_id}}\",\n  \"members\": [\n    {\n      \"Email\": \"newfriend@example.com\",\n      \"Name\": \"New Friend\"\n    }\n  ]\n}"
						},
						"url": {
							"raw": "{{base_url}}/stream__updateMembers",
							"host": ["{{base_url}}"],
							"path": ["stream__updateMembers"]
						},
						"description": "Updates or adds new members to existing stream"
					}
				}
			]
		},
		{
			"name": "3. Question & Answer System",
			"item": [
				{
					"name": "Create Question Post",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "page_id",
								"value": "{{page_id}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"MediaType\": \"Notes\",\n  \"PostType\": \"QuestionPost\",\n  \"Content\": \"What's your favorite memory with John?\",\n  \"PostStatement\": \"What's your favorite memory with John?\",\n  \"IsOnetimeStream\": false,\n  \"IsPrivateQuestionPost\": false,\n  \"SurpriseSelectedWords\": \"memory, favorite, John\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/media/addMedia",
							"host": ["{{base_url}}"],
							"path": ["media", "addMedia"]
						},
						"description": "Creates a question post for members to answer"
					}
				},
				{
					"name": "Create Answer Post",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "page_id",
								"value": "{{page_id}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"MediaType\": \"Notes\",\n  \"PostType\": \"AnswerPost\",\n  \"Content\": \"My favorite memory is when we went hiking together last summer. John was so encouraging when I was struggling with the steep trail!\",\n  \"PostStatement\": \"My favorite memory is when we went hiking together last summer. John was so encouraging when I was struggling with the steep trail!\",\n  \"QuestionPostId\": \"{{question_post_id}}\",\n  \"IsOnetimeStream\": false,\n  \"SurpriseSelectedWords\": \"hiking, summer, encouraging, trail\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/media/addMedia",
							"host": ["{{base_url}}"],
							"path": ["media", "addMedia"]
						},
						"description": "Creates an answer post responding to a question"
					}
				},
				{
					"name": "Create Video Answer Post",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "page_id",
								"value": "{{page_id}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"MediaType\": \"Video\",\n  \"PostType\": \"AnswerPost\",\n  \"Content\": \"Happy Birthday John! Here's my video message for you.\",\n  \"PostStatement\": \"Happy Birthday John! Here's my video message for you.\",\n  \"QuestionPostId\": \"{{question_post_id}}\",\n  \"IsOnetimeStream\": true,\n  \"MediaURL\": \"path/to/video/file.mp4\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/media/addMedia",
							"host": ["{{base_url}}"],
							"path": ["media", "addMedia"]
						},
						"description": "Creates a video answer post for birthday wishes"
					}
				},
				{
					"name": "Mark Answer as Complete",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"StreamId\": \"{{stream_id}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/addAnswerFinishStatus",
							"host": ["{{base_url}}"],
							"path": ["addAnswerFinishStatus"]
						},
						"description": "Marks that a member has completed answering all questions"
					}
				},
				{
					"name": "Get Answer Stage Status",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"StreamId\": \"{{stream_id}}\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/getAnswerStageStatus",
							"host": ["{{base_url}}"],
							"path": ["getAnswerStageStatus"]
						},
						"description": "Checks the completion status of answers for a stream"
					}
				}
			]
		},
