		{
			"name": "5. Birthday Delivery System",
			"item": [
				{
					"name": "Create Posts on Event Day (Internal API)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"capsuleId\": \"{{capsule_id}}\",\n  \"SessionUser\": {\n    \"_id\": \"{{birthday_person_id}}\",\n    \"Name\": \"John Doe\",\n    \"Email\": \"john@example.com\",\n    \"Birthdate\": \"1990-05-15\",\n    \"AllFoldersId\": \"{{folder_id}}\",\n    \"AllPagesId\": \"{{page_id}}\"\n  },\n  \"MonthFor\": \"M12\",\n  \"Frequency\": \"medium\",\n  \"EmailTemplate\": \"PracticalThinker\",\n  \"IsStreamPaused\": false,\n  \"EmailSubject\": \"Your Birthday Surprise is Here!\",\n  \"IsOnetimeStream\": false,\n  \"IsOnlyPostImage\": false\n}"
						},
						"url": {
							"raw": "{{base_url}}/capsules/createPostsOnEventDay_INTERNAL_API",
							"host": ["{{base_url}}"],
							"path": ["capsules", "createPostsOnEventDay_INTERNAL_API"]
						},
						"description": "Triggers the birthday delivery system to schedule posts for the birthday person"
					}
				},
				{
					"name": "Check Post Streams",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
